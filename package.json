{
  "name": "fast-menubar",
  "version": "1.0.0",
  "description": "Take a speedtest from your Mac's Menu Bar",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron app",
    "prepackage": "npm run clean",
    "package": "build --dir",
    "prerelease": "npm run clean",
    "release": "build",
    "clean": "rimraf dist"
  },
  "build": {
    "appId": "com.netflix.fast",
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "author": "Rahul Doshi <rahuld223@yahoo.com>",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^15.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doshprompt/fast-menubar.git"
  },
  "keywords": [
    "fast.com",
    "electron",
    "menubar",
    "macos"
  ],
  "bugs": {
    "url": "https://github.com/doshprompt/fast-menubar/issues"
  },
  "homepage": "https://github.com/doshprompt/fast-menubar#readme"
}
